{
  "name": "cma",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "export NODE_ENV=development",
    "start": "babel-node ./src/bin/www",
    "pretest": "export NODE_ENV=test",
    "test": "mocha",
    "predist": "export NODE_ENV=production rm -rf dist && mkdir ./dist/bin && npm run dist:www",
    "dist:www": "babel ./src/bin/www --out-file ./dist/bin/www",
    "dist": "babel src -d dist"
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "moment": "^2.18.1",
    "mongoose": "^4.9.1",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "pug": "~2.0.0-beta10",
    "request": "^2.81.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.18.0",
    "mocha": "^3.2.0"
  }
